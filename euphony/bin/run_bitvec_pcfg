#!/bin/bash

PYPATH="$EUPHONY_HOME/thirdparty/Python-3.5.1/python"
export Z3_LIBRARY_PATH="$EUPHONY_HOME/thirdparty/z3/build/python"

if [ -z "$PYPATH" ]; then
	echo "python3 not found"
else
	PYTHONPATH=$EUPHONY_HOME/thirdparty/libeusolver/build:$EUPHONY_HOME/thirdparty/z3/build/python "$PYPATH" $EUPHONY_HOME/bin/benchmarks.py -sphog $EUPHONY_HOME/bin/trained/pcfg_bv "$1"
fi
